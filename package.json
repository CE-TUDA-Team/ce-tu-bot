{
  "name": "ce-tu-bot",
  "version": "2.0.0",
  "description": "Discord bot written in Typescript and discord.js hosted on Heroku",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prestart:dev": "npm run build",
    "start:dev": "nodemon build/index.js",
    "prebuild": "rimraf ./build",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CE-TUDA-Team/ce-tu-bot.git"
  },
  "keywords": [
    "discord",
    "typescript",
    "bot",
    "heroku"
  ],
  "author": "Niklas Beck, Paul Hollmann",
  "license": "none",
  "bugs": {
    "url": "https://github.com/CE-TUDA-Team/ce-tu-bot"
  },
  "homepage": "https://github.com/CE-TUDA-Team/ce-tu-bot",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "nodemon": "latest",
    "ts-node": "latest"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.23.1",
    "discord.js": "latest",
    "dotenv": "latest",
    "rimraf": "latest",
    "typescript": "latest"
  }
}
