{
  "name": "ce-tu-bot",
  "version": "2.0.0",
  "description": "Discord bot written in Typescript and discord.js hosted on Heroku",
  "main": "build/index.js",
  "type": "commonjs",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "npm run build && node build/index.js",
    "dev": "npm run build && ts-node src/index.ts",
    "build": "npm update && tsc",
    "lint": "eslint .",
    "clean": "rm package-lock.json && rm -rf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CE-TUDA-Team/ce-tu-bot.git"
  },
  "keywords": [
    "discord",
    "typescript",
    "bot",
    "heroku"
  ],
  "author": "Niklas Beck, Paul Hollmann",
  "license": "none",
  "bugs": {
    "url": "https://github.com/CE-TUDA-Team/ce-tu-bot"
  },
  "homepage": "https://github.com/CE-TUDA-Team/ce-tu-bot",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "ts-node": "latest"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.23.1",
    "discord.js": "13.2.0",
    "dotenv": "latest",
    "rimraf": "latest",
    "typescript": "latest"
  }
}
